import java.util.Scanner;
class main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int m=sc.nextInt();
        int P=0;
        for(int i=0;i<n;i++)
        {
            P=P+m;
        }
        System.out.println(P);
    }
}
